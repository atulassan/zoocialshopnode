                </div>
                <div id="attr_lebensphase9" class="attr_lebensphase filterToggleOpen">
                    <input type="text" class="form-control" placeholder="suche" id="myInput9">
                    <% if(futterartData != null && futterartData.length > 0) { %>
                      <ul>
                          <% futterartData.forEach(function(futterart, el) { %>
                              <li>
                                <div class="form-check">
                                  <input type="checkbox" name="futterart" data-label="<%= futterart.name.replace(/&amp;/g, "&") %>|<%= futterart.id %>" class="form-check-input filter-lebensphase" value="<%= futterart.id %>" id="<%= futterart.id %>">
                                  <label class="form-check-label" for="<%= futterart.id %>"><%= futterart.name.replace(/&amp;/g, "&") %></label>
