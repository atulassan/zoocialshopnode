                      <% if(tags != null && tags.length > 0) { %>
                        <% tags.forEach(function(tag, el) { %>
                          <% if(parseInt(tag.count) > 0) { %>
                            <li>
                              <div class="form-check">
                                <input type="checkbox" name="brand" data-label="<%= tag.name.replace(/&amp;/g, "&") %>|<%= tag.id %>" class="form-check-input filter-lebensphase" value="<%= tag.id %>" id="<%= tag.id %>">
                                <label class="form-check-label" for="<%= tag.id %>"><%= tag.name.replace(/&amp;/g, "&") %></label>
