                            <li>
                              <div class="form-check">
                                <input type="checkbox" name="grosse" data-label="<%= grosse.name.replace(/&amp;/g, "&") %>|<%= grosse.id %>" class="form-check-input filter-lebensphase" value="<%= grosse.id %>" id="<%= grosse.id %>">
                                <label class="form-check-label" for="<%= grosse.id %>"><%= grosse.name.replace(/&amp;/g, "&") %></label>
                              </div>
                            </li>
                        <% }); %>
                    </ul>
                  <% } %>
                    <button type="button" class="schliessen">Schliessen</button>
                </div>
            </div>
          </li>
