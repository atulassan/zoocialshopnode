    </ul>
  </div>
  </form>

  <% if(urlFilters.length > 0) { %>
  <div class="filter-applied">
    <% urlFilters.forEach(function(data, el) { %>
      <span class="c-f-item"><%= data.name %><a href="javascript:void(0)" data-id="<%= data.id %>" class="o-i-close-applied-filter"><i class="fa fa-times"></i></a></span>
    <% }); %>
    <a id="clearfilters" href="javascript:void(0)" class="inner-gradient-bg f-clear">löschen</a>
  </div>
  <% } else { %>
    <div class="filter-applied"></div>
  <% } %>
</div>