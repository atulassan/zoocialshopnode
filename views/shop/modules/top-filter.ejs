<div class="filterFixed">
  <div class="mFilterBtns">
    <div class="btn-group">
      <a class="mobileFiterOpen" href="javascript:;"><img src="/images/svg/menu-bar.svg" alt=""></a>
      <a  class="mobileTopFilter" data-toggle="collapse" href="#show-filter" aria-expanded="true">
        Filter <i class="fa fa-filter" aria-hidden="true"></i></a>
    </div>
  </div>
  <form id="filters" name="filters" method="post">
  <div class="collapse navbar-collapse" id="show-filter">
    <ul class="newFilter">
          <li>
            <div class="block-content Filter-toggle-content">
                <div class="FilterToggle">
                    <label class="amshopby_label">Lebensphase <i class="fa fa-angle-down"></i></label>
                    <span class="special_border"></span>
                </div>
                <div id="attr_lebensphase1" class="attr_lebensphase filterToggleOpen">
                  <input type="text" class="form-control" placeholder="suche" id="myInput1">
                    <% if(lebensphaseData != null && lebensphaseData.length > 0) { %>
                      <ul>
                          <% lebensphaseData.forEach(function(lebensphase, el) { %>
                              <li>
                                <div class="form-check">
                                  <input type="checkbox" name="lebensphase" data-label="<%= lebensphase.name.replace(/&amp;/g, "&") %>|<%= lebensphase.id %>" class="form-check-input filter-lebensphase" value="<%= lebensphase.id %>" id="<%= lebensphase.id %>">
                                  <label class="form-check-label" for="<%= lebensphase.id %>"><%= lebensphase.name.replace(/&amp;/g, "&") %></label>
                                </div>
                              </li>
                            
                          <% }); %>
                      </ul>
                    <% } %>
                    <button type="button" class="schliessen">Schliessen</button>
                </div>
            </div>
          </li>
          <li>
            <div class="block-content Filter-toggle-content">
                <div class="FilterToggle">
                    <label class="amshopby_label">Grösse <i class="fa fa-angle-down"></i></label>
                    <span class="special_border"></span>
                </div>
                <div id="attr_lebensphase2" class="attr_lebensphase filterToggleOpen">
